// Diagrama UML de la Base de Datos
digraph {
	Pedido [label="Pedido
(id_pedido, posicion, tipo, pedido_tipoimp, descripcion, fecha_creacion, fecha_actualizacion, estado_tramitacion_id, creador_id, contrato_id, cesta_id, moneda_id, id_proveedor)"]
	HistorialPedido [label="HistorialPedido
(id, id_pedido, estado_anterior, estado_nuevo, estado_aprobacion, fecha_cambio, id_usuario, id_transicion)"]
	Transicion [label="Transicion
(id, estado_origen, estado_destino, estado_origen_id, estado_destino_id)"]
	Estado [label="Estado
(id, nombre, tipo, requiere_aprobacion)"]
	Usuario [label="Usuario
(id, nombre, email, rol)"]
	Proveedor [label="Proveedor
(id_proveedor, nombre, contacto, email, estado, fecha_registro)"]
	Oferta [label="Oferta
(id, id_pedido, id_proveedor, precio_propuesto, estado_oferta, fecha_oferta)"]
	Facturacion [label="Facturacion
(id, id_pedido, monto_total, estado_factura, fecha_emision, fecha_pago)"]
	Contrato [label="Contrato
(id_contrato, descripcion, fecha_creacion, fecha_inicio, fecha_fin, proveedor_id, cesta_general_id, moneda_id)"]
	CestaGeneral [label="CestaGeneral
(id_cesta_general, nombre, descripcion, fecha_creacion, moneda_id, proveedor_id)"]
	Cesta [label="Cesta
(id_cesta, nombre, tipo_compra, presupuesto, usuario_sap_id, contrato_id, proveedor_id, moneda_id, fecha_creacion)"]
	Factura [label="Factura
(id_factura, fecha_creacion, pedido_id, proveedor_id, moneda_id, valor)"]
	Moneda [label="Moneda
(id_moneda, codigo, nombre)"]
	Pedido -> HistorialPedido [label="1 - *"]
	HistorialPedido -> Transicion [label="N - 1"]
	HistorialPedido -> Usuario [label="N - 1"]
	Pedido -> Estado [label="N - 1"]
	Pedido -> Usuario [label="N - 1"]
	Pedido -> Proveedor [label="N - 1"]
	Pedido -> Contrato [label="N - 1"]
	Pedido -> Cesta [label="N - 1"]
	Pedido -> Moneda [label="N - 1"]
	Pedido -> Oferta [label="1 - *"]
	Pedido -> Facturacion [label="1 - 1"]
	Contrato -> Proveedor [label="N - 1"]
	Contrato -> CestaGeneral [label="N - 1"]
	Contrato -> Moneda [label="N - 1"]
	CestaGeneral -> Proveedor [label="N - 1"]
	CestaGeneral -> Moneda [label="N - 1"]
	Cesta -> Usuario [label="N - 1"]
	Cesta -> Contrato [label="N - 1"]
	Cesta -> Proveedor [label="N - 1"]
	Cesta -> Moneda [label="N - 1"]
	Factura -> Pedido [label="N - 1"]
	Factura -> Proveedor [label="N - 1"]
	Factura -> Moneda [label="N - 1"]
}
